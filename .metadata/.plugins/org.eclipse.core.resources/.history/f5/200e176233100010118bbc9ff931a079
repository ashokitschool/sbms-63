package in.ashokit;

public class Car {

	private Engine eng=null;

	public Car() {
		System.out.println("Car :: Constructor");
	}

	public void drive() {
		boolean status = eng.start();
		if (status) {
			System.out.println("Journey Started..");
		} else {
			System.out.println("Engine Trouble...");
		}
	}
}
